{
  "name": "vue-notus",
  "homepage": "https://demos.creative-tim.com/vue-notus/",
  "version": "1.1.0",
  "description": "Vue Notus - A free Tailwind CSS and VueJS UI Kit and Admin by Creative Tim.",
  "repository": "https://github.com/creativetimofficial/vue-notus",
  "license": "MIT",
  "engines": {
    "node": "16.0.0"
  },
  "scripts": {
    "init": "git clone https://github.com/awaisnazir21/car-connect.git && rm -rf node_modules/ && rm -rf package-lock.json && npm install && tailwind build src/assets/styles/index.css -o src/assets/styles/tailwind.css && npm run serve",
    "dev": "vue-cli-service build --report && vue-cli-service lint",
    "start": "vue-cli-service build --report && vue-cli-service lint && node server.js",
    "git": "git add . && git commit -m 'main' && git push -u origin main && git push -u origin2 main",
    "git-version": "git add . && git commit -m '1.18' && git checkout -b 1.18 && git push -u origin 1.18 && git push -u origin2 1.18"
  },
  "dependencies": {
    "@fortawesome/fontawesome-free": "5.15.3",
    "@tailwindcss/forms": "0.2.1",
    "@vue/compiler-sfc": "3.0.7",
    "accordion-js": "^3.1.1",
    "axios": "^0.21.1",
    "cookie-parser": "^1.4.5",
    "cookies": "^0.8.0",
    "core-js": "3.9.1",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-status-monitor": "^1.3.3",
    "firebase": "^8.5.0",
    "git": "^0.1.5",
    "gridjs": "^4.0.0",
    "gulp": "4.0.2",
    "gulp-append-prepend": "1.0.8",
    "jquery": "^3.6.0",
    "json-table-converter": "^0.3.0",
    "multer": "^1.4.2",
    "nod-validate": "^2.0.12",
    "nodemailer": "^6.6.0",
    "sticky-js": "^1.3.0",
    "tabby-js": "^0.1.1",
    "tailwindcss": "2.0.4",
    "vue": "3.0.7",
    "vue-axios": "^3.2.4",
    "vue-router": "4.0.5",
    "vue-scrollto": "^2.20.0",
    "vue3-cookies": "^1.0.1"
  },
  "devDependencies": {
    "@babel/core": "7.13.10",
    "@babel/eslint-parser": "7.13.10",
    "@types/node": "^15.6.1",
    "@vue/cli-plugin-babel": "5.0.0-alpha.7",
    "@vue/cli-plugin-eslint": "5.0.0-alpha.7",
    "@vue/cli-service": "5.0.0-alpha.7",
    "autoprefixer": "10.2.5",
    "eslint": "7.22.0",
    "eslint-plugin-vue": "7.7.0",
    "niui-npm": "^2.2.4",
    "postcss": "8.2.8",
    "vue-template-compiler": "2.6.12"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "@babel/eslint-parser"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
